@{
    var direction = EPMS.Web.Resources.Shared.Common.TextDirection;
}
<div id="warhouseSelector" class="modal hide fade treeSelector">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h3>Warhouse</h3>
    </div>
    <div class="modal-body ">
        <form class="form-horizontal warhouseSelection">
            <input id='WarehouseModalIndex' type='hidden' />
            <div id="Simple_Multiple_Select_Box" class="control-group row-fluid">
                <div class="span4">
                    <label class="control-label" for="MaritalStatus">Select one or more Warhouses </label>
                </div>
                <div class="span8">
                    <div class="controls">
                        <select multiple name="simpleMulti" id="simpleMultiWarehouse">
                        </select>
                    </div>
                </div>

            </div>
            <div class="control-group row-fluid">
                <div class="span12 span-inset">
                    <button type="button" class="btn addWar">Add Warhouse</button>
                </div>
            </div>
        </form>
    </div>
    <div class="modal-footer">
        <a href="javascript:;" class="btn btn-primary">Add</a>
        <a href="javascript:;" class="btn" data-dismiss="modal">Cancel</a>
    </div>
</div>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!-- Navigation Bar Start-->
<div class="breadcrumb-container">
    <ul class="xbreadcrumbs">
        <li>
            <a href="~/Dashboard/Index">
                <i class="icon-photon home"></i>
            </a>
        </li>
        <li class="current">
            @EPMS.WebModels.Resources.PMS.Project.ProjectsList
        </li>
    </ul>
</div>
<!-- Navigation Bar End-->
<!-- Page Heading Start-->
<header>
    <i class="icon-big-notepad"></i>
    <h2><small>@EPMS.WebModels.Resources.PMS.Project.ListPageHeading</small></h2>
</header>
<!-- Page Heading End-->
<!--Notifications begin-->
@Html.Partial("_Alert")
<!--Notifications end-->

<form class="form-horizontal" id="validation_form">
    <div class="container-fluid">

        <div class="control-group row-fluid">
            <div class="span3">
                <label class="control-label" for="jobTitle">Project Name </label>
            </div>
            <div class="span2">
                <div class="controls">
                    <select>
                        <option>All</option>
                        <option>Website</option>
                        <option>Designing</option>
                    </select>
                </div>
            </div>

        </div>

        <!--Input Field end-->







        <div class="control-group row-fluid">
            <div class="span3 span-inset">
                <button type="button" class="btn">Generate Report</button>
            </div>

        </div>
    </div><!-- end container -->




</form>
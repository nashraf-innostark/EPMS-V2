<script>
    $(document).ready(function () {
        if ('@direction' == "rtl") {
            $('html, body').css('direction', 'rtl');
            $('html, body').css('text-align', 'right');
        }
        $('#tableSortableResMed,#tableSortableResMed2').dataTable({
            "sPaginationType": "bootstrap",
            "fnInitComplete": function () {
                $(".dataTables_wrapper select").select2({
                    dropdownCssClass: 'noSearch'
                });
            }
        });
    });
    $("#simpleSelectBox").select2({
        dropdownCssClass: 'noSearch'
    });
    $().ready(function () {
        $("#selectBoxFilter,selectBoxFilter2").select2();
    });
    $(".uniformCheckbox").uniform();
    $("#AcceptAgreement").change(function () {
        if ($("#AcceptAgreement").is(':checked')) {
            $('input[type="submit"]').removeAttr('disabled');
        } else {
            $('input[type="submit"]').attr('disabled', 'disabled');
        }
    });
</script>
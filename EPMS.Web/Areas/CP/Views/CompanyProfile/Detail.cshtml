@model EPMS.Web.ViewModels.CompanyProfile.CompanyProfileViewModel

@{
    ViewBag.Title = @EPMS.Web.Resources.CP.Profile.CompanyProfile;
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.MessageVM = TempData["message"];
}

<div class="breadcrumb-container">
    <ul class="xbreadcrumbs">
        <li>
            <a href="~/Dashboard/Index">
                <i class="icon-photon home"></i>
            </a>
        </li>
        <li class="current">
            <a href="javascript:;">@EPMS.Web.Resources.CP.Profile.CompanyProfile</a>
        </li>
    </ul>
</div>
<header>
    <i class="icon-big-notepad"></i>
    <h2><small>@ViewBag.Title</small></h2>
    @if (!(string.IsNullOrEmpty(ViewBag.LogoPath)))
    {
        <img class="employeeImg" src='@Url.Content(ViewBag.LogoPath)' alt="user" />
    }
    else
    {
        <img class="employeeImg" alt="Logo" />
    }
</header>

<!--Notifications begin-->
@Html.Partial("_Alert")
<!--Notifications end-->

<div class="form-horizontal">
    <div class="container-fluid">
        <!--Company Logo-->
        <div id="Drag_And_Drop_Upload" class="control-group row-fluid">
            <div class="span3">
                <label class="control-label" for="inputField">
                    @EPMS.Web.Resources.CP.Profile.CompanyLogo<a href="javascript:;" class="bootstrap-tooltip" data-placement="top" data-original-title="file size shouldn't exceed 1MB"><i class="icon-photon info-circle"></i></a>
                </label>
            </div>
            <div class="span9 row-fluid">
                <div class="upload-boxes row-fluid">
                    <div class="span3" id="upload"></div>
                    <div class="span9" id="uploaded"></div>
                </div>
            </div>
        </div>
        <!--Tabs-->
        <div class="control-group row-fluid">
            <ul class="nav nav-tabs" id="myTab">
                <li class="active"><a data-toggle="tab" href="#tab-1">@EPMS.Web.Resources.CP.Profile.CompanyInformation</a></li>
                <li class=""><a data-toggle="tab" href="#tab-2">@EPMS.Web.Resources.CP.Profile.CompanyDocuments</a></li>
                <li class=""><a data-toggle="tab" href="#tab-3">@EPMS.Web.Resources.CP.Profile.LicenseInformation</a></li>
                <li class=""><a data-toggle="tab" href="#tab-4">@EPMS.Web.Resources.CP.Profile.BankInformation</a></li>
                <li class=""><a data-toggle="tab" href="#tab-5">@EPMS.Web.Resources.CP.Profile.SocialMedia</a></li>
            </ul>
            <div class="tab-content">
                <!--Company Information-->
                <div id="tab-1" class="tab-pane fade active in">

                        @Html.HiddenFor(m => m.CompanyProfile.CompanyId)
                        @Html.HiddenFor(m => m.TabId, new { @class = "TabId" })
                        @Html.HiddenFor(model => model.CompanyProfile.CompanyLogoPath, new { @id = "CompanyLogoPath" })

                        <!--Input Field begin-->
                        <div class="control-group row-fluid">
                            <div class="span3">
                                <label class="control-label">@EPMS.Web.Resources.CP.Profile.Name</label>
                            </div>
                            <div class="span9">
                                <div class="controls">
                                    @Html.TextBoxFor(m => m.CompanyProfile.CompanyNameE, new { id = "CompanyNameE" })
                                </div>
                            </div>
                        </div>
                        <div class="control-group row-fluid">
                            <div class="span3">
                                <label class="control-label">@EPMS.Web.Resources.CP.Profile.NameAr</label>
                            </div>
                            <div class="span9">
                                <div class="controls">
                                    @Html.TextBoxFor(m => m.CompanyProfile.CompanyNameA, new { id = "CompanyNameA" })
                                </div>
                            </div>
                        </div>
                        <div class="control-group row-fluid">
                            <div class="span3">
                                <label class="control-label">@EPMS.Web.Resources.CP.Profile.Website</label>
                            </div>
                            <div class="span9">
                                <div class="controls">
                                    @Html.TextBoxFor(m => m.CompanyProfile.CompanyWebsite, new { id = "CompanyWebsite" })
                                </div>
                            </div>

            @Html.HiddenFor(m => m.BankIbanNo, new { @class = "BankIbanNo" })
            @Html.HiddenFor(m => m.BankMobileNo, new { @class = "BankMobileNo" })
            <div class="popup-ct">
                <div class="popup-custom row-fluid">
                    <div class="span3">
                        <label class="control-label">Select Contact</label>
                    </div>
                    <div class="span9">
                        <div class="controls">
                            @*@Html.DropDownList("NameE", new SelectList(ViewBag.ContactList, "", "MobileNumber"))*@
                            @Html.ListBoxFor(m => m.MobileNo, new MultiSelectList(Model.List, "MobileNumber", "NameE"))
                        </div>
                    </div>
                </div>
                <div class="">
                    <button type="submit">Send</button>
                </div>
            </div>
        }
    </div>
    </div>


<script>
    $().ready(function () {

        $("#ProfileForm").validate({
            errorElement: "span",
            errorClass: "error",
            onclick: true,
            rules: {
                "req": {
                    required: true
                },
                "numbers": {
                    required: false,
                    digits: true
                },
                "email": {
                    email: true
                },
                "url": {
                    url: true
                },
                "date": {
                    date: true
                }
            }
        });

        //Set Arabic and Gregorian Dates
        $("#CommercialRegisterIssueDateAr").val(ConvertDates($("#CommercialRegisterIssueDate").val(), "gregorian", "islamic"));
        $("#CommercialRegisterExpiryDateAr").val(ConvertDates($("#CommercialRegisterExpiryDate").val(), "gregorian", "islamic"));
        $("#InsuranceCertificateIssueDateAr").val(ConvertDates($("#InsuranceCertificateIssueDate").val(), "gregorian", "islamic"));
        $("#InsuranceCertificateExpiryDateAr").val(ConvertDates($("#InsuranceCertificateExpiryDate").val(), "gregorian", "islamic"));
        $("#ChamberCertificateIssueDateAr").val(ConvertDates($("#ChamberCertificateIssueDate").val(), "gregorian", "islamic"));
        $("#ChamberCertificateExpiryDateAr").val(ConvertDates($("#ChamberCertificateExpiryDate").val(), "gregorian", "islamic"));
        $("#IncomeAndZakaCertificateIssueDateAr").val(ConvertDates($("#IncomeAndZakaCertificateIssueDate").val(), "gregorian", "islamic"));
        $("#IncomeAndZakaCertificateExpiryDateAr").val(ConvertDates($("#IncomeAndZakaCertificateExpiryDate").val(), "gregorian", "islamic"));
        $("#SaudilizationCertificateIssueDateAr").val(ConvertDates($("#SaudilizationCertificateIssueDate").val(), "gregorian", "islamic"));
        $("#SaudilizationCertificateExpiryDateAr").val(ConvertDates($("#SaudilizationCertificateExpiryDate").val(), "gregorian", "islamic"));

        var companyId = $('#CompanyProfile_CompanyId').val();
        if (companyId == 0) {
            $('.' + 'tbdisable' + ':input').attr('disabled', true);
            $(".saveButton").hide();
        }

        var url = "/CP/CompanyProfile/UploadLogo";
        var errors = "";
        $('#upload').mfupload({
            type: 'jpg,png,tif,jpeg', // type: '', for all types
            maxsize: 1, //file size in MBs
            post_upload: url,
            folder: "./",
            ini_text: "@EPMS.Web.Resources.CP.Profile.UploadBox",
            over_text: "Drop Here",
            over_col: '#666666',
            over_bkcol: '#f0f0f0',

            init: function () {
                $("#uploaded").empty();
            },

            start: function (result) {
                $("#uploaded").append("<div id='FILE" + result.fileno + "' class='files'>" + result.filename + "<div class='progress progress-info progress-thin'><div class='bar' id='PRO" + result.fileno + "'></div></div></div>");
            },

            loaded: function (result) {
                $("#PRO" + result.fileno).remove();
                $("#FILE" + result.fileno).html("Uploaded: " + result.filename + " (" + result.size + ")");
                $('#CompanyLogoPath').val(result.filename);
            },

            progress: function (result) {
                $("#PRO" + result.fileno).css("width", result.perc + "%");
            },

            error: function (error) {
                errors += error.filename + ": " + error.err_des + "\n";
            },

            completed: function () {
                if (errors != "") {
                    alert(errors);
                    errors = "";
                }
            }
        });
    });

    function SetTabId(id) {
        $('.TabId').val(id);
        $('.CompanyId').val('@ViewBag.CompanyId');
    };
    $(function () {
        $('a[rel*=leanModal]').leanModal({ top: 80, closeButton: ".btnSelect,.modal_close" });
    });

    function SetDetail(id) {
        $('.BankName').val($('#BankName' + id).val());
        $('.BankNameAr').val($('#BankNameArabic' + id).val());
        $('.BankAccountNo').val($('#BankAccountNumber' + id).val());
        $('.BankIbanNo').val($('#BankIBANNumber' + id).val());
        $('.BankMobileNo').val($('#BankMobileNumber' + id).val());
        $("#go").click();
    }
    function hide() {
        $("#popup").hide();
    }
</script>
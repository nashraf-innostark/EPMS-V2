                            <div class="span3">
                                <label class="control-label">@EPMS.Web.Resources.CP.Profile.NameAr</label>
                            </div>
                            <div class="span9">
                                <div class="controls">
                                    @Html.TextBoxFor(m => m.CompanyProfile.CompanyNameA, new { id = "CompanyNameA" })
                                </div>

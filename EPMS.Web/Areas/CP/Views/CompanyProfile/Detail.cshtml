                            <div class="span9">
                                <div class="controls">
                                    @Html.PasswordFor(m => m.CompanySocial.Social2Password, new { id = "Social2Password", @class = "tbdisable" })
                                </div>
                            </div>
                        </div>
                        <div class="control-group row-fluid">
                            <div class="span3 span-inset">
                                <button id="savebtn" class="btn saveButton" type="submit" onclick="SetTabId(5)">@EPMS.Web.Resources.CP.Profile.SaveButton</button>
                            </div>
                        </div>
                    }
                </div>

            </div>
        </div>
        <!--Tabs Content-->
    </div>
</div>

<a id="go" rel="leanModal" name="popup" href="#popup"></a>
<div id="popup" class="pops-custom">
    <div class="popup-header">
        <button type="button" class="close" onclick="hide()">×</button>
        <h3 id="popupHeader">Send SMS</h3>
    </div>
    <div class="form-horizontal">
        @using (Html.BeginForm("SendSMS", "CompanyProfile", FormMethod.Post, new { @id = "SMSForm", @class = "form-horizontal", role = "form" }))
        {
            @Html.HiddenFor(m => m.BankName, new { @class = "BankName" })
            @Html.HiddenFor(m => m.BankNameAr, new { @class = "BankNameAr" })
            @Html.HiddenFor(m => m.BankAccountNo, new { @class = "BankAccountNo" })
            @Html.HiddenFor(m => m.BankIbanNo, new { @class = "BankIbanNo" })
            @Html.HiddenFor(m => m.BankMobileNo, new { @class = "BankMobileNo" })
            <div class="popup-ct">
                <div class="popup-custom row-fluid">
                    <div class="span3">
                        <label class="control-label">Select Contact</label>
                    </div>
                    <div class="span9">
                        <div class="controls">
                            @Html.ListBoxFor(m => m.MobileNo, new MultiSelectList(Model.List, "MobileNumber", "NameE"))
                        </div>
                    </div>
                </div>
                <div class="">
                    <button type="submit">Send</button>
                </div>
            </div>
        }
    </div>
</div>

<script src="~/Scripts/plugins/jquery.blockUI.js"></script>
<script>
    var siteUrl;
    $().ready(function () {
        siteUrl = $('#siteURL').val();
        $("#ProfileForm").validate({
            errorElement: "span",
            errorClass: "error",
            onclick: true,
            rules: {
                "req": {
                    required: true
                },
                "numbers": {
                    required: false,
                    digits: true
                },
                "email": {
                    email: true
                },
                "url": {
                    url: true
                },
                "date": {
                    date: true
                }
            }
        });

        //Set Arabic and Gregorian Dates
        if ($("#CommercialRegisterIssueDate").val() != null && $("#CommercialRegisterIssueDate").val() != "") {
            $("#CommercialRegisterIssueDateAr").val(ConvertDates($("#CommercialRegisterIssueDate").val(), "gregorian", "islamic"));
        }
        if ($("#CommercialRegisterExpiryDate").val() != null && $("#CommercialRegisterExpiryDate").val() != "") {
            $("#CommercialRegisterExpiryDateAr").val(ConvertDates($("#CommercialRegisterExpiryDate").val(), "gregorian", "islamic"));
        }
        if ($("#InsuranceCertificateIssueDate").val() != null && $("#InsuranceCertificateIssueDate").val() != "") {
            $("#InsuranceCertificateIssueDateAr").val(ConvertDates($("#InsuranceCertificateIssueDate").val(), "gregorian", "islamic"));
        }
        if ($("#InsuranceCertificateExpiryDate").val() != null && $("#InsuranceCertificateExpiryDate").val() != "") {
            $("#InsuranceCertificateExpiryDateAr").val(ConvertDates($("#InsuranceCertificateExpiryDate").val(), "gregorian", "islamic"));
        }
        if ($("#ChamberCertificateIssueDate").val() != null && $("#ChamberCertificateIssueDate").val() != "") {
            $("#ChamberCertificateIssueDateAr").val(ConvertDates($("#ChamberCertificateIssueDate").val(), "gregorian", "islamic"));
        }
        if ($("#ChamberCertificateExpiryDate").val() != null && $("#ChamberCertificateExpiryDate").val() != "") {
            $("#ChamberCertificateExpiryDateAr").val(ConvertDates($("#ChamberCertificateExpiryDate").val(), "gregorian", "islamic"));
        }
        if ($("#IncomeAndZakaCertificateIssueDate").val() != null && $("#IncomeAndZakaCertificateIssueDate").val() != "") {
            $("#IncomeAndZakaCertificateIssueDateAr").val(ConvertDates($("#IncomeAndZakaCertificateIssueDate").val(), "gregorian", "islamic"));
        }
        if ($("#IncomeAndZakaCertificateExpiryDate").val() != null && $("#IncomeAndZakaCertificateExpiryDate").val() != "") {
            $("#IncomeAndZakaCertificateExpiryDateAr").val(ConvertDates($("#IncomeAndZakaCertificateExpiryDate").val(), "gregorian", "islamic"));
        }
        if ($("#SaudilizationCertificateIssueDate").val() != null && $("#SaudilizationCertificateIssueDate").val() != "") {
            $("#SaudilizationCertificateIssueDateAr").val(ConvertDates($("#SaudilizationCertificateIssueDate").val(), "gregorian", "islamic"));
        }
        if ($("#SaudilizationCertificateExpiryDate").val() != null && $("#SaudilizationCertificateExpiryDate").val() != "") {
            $("#SaudilizationCertificateExpiryDateAr").val(ConvertDates($("#SaudilizationCertificateExpiryDate").val(), "gregorian", "islamic"));
        }
        if ($("#LicenseStartDate").val() != null && $("#LicenseStartDate").val() != "") {
            $("#LicenseStartDateAr").val(ConvertDates($("#LicenseStartDate").val(), "gregorian", "islamic"));
        }
        if ($("#LicenseEndDate").val() != null && $("#LicenseEndDate").val() != "") {
            $("#LicenseEndDateAr").val(ConvertDates($("#LicenseEndDate").val(), "gregorian", "islamic"));
        }

        var companyId = $('#CompanyProfile_CompanyId').val();
        if (companyId == 0) {
            $('.' + 'tbdisable' + ':input').attr('disabled', true);
            $(".saveButton").hide();
        }

        var url = siteUrl + "/CP/CompanyProfile/UploadLogo";
        var errors = "";
        $('#upload').mfupload({
            type: 'jpg,png,tif,jpeg', // type: '', for all types
            maxsize: 1, //file size in MBs
            post_upload: url,
            folder: "./",
            ini_text: "@EPMS.Web.Resources.CP.Profile.UploadBox",
            over_text: "Drop Here",
            over_col: '#666666',
            over_bkcol: '#f0f0f0',

            init: function () {
                $("#uploaded").empty();
            },

            start: function (result) {
                $("#uploaded").append("<div id='FILE" + result.fileno + "' class='files'>" + result.filename + "<div class='progress progress-info progress-thin'><div class='bar' id='PRO" + result.fileno + "'></div></div></div>");
            },

            loaded: function (result) {
                $("#PRO" + result.fileno).remove();
                $("#FILE" + result.fileno).html("Uploaded: " + result.filename + " (" + result.size + ")");
                $('#CompanyLogoPath').val(result.filename);
            },

            progress: function (result) {
                $("#PRO" + result.fileno).css("width", result.perc + "%");
            },

            error: function (error) {
                errors += error.filename + ": " + error.err_des + "\n";
            },

            completed: function () {
                if (errors != "") {
                    alert(errors);
                    errors = "";
                }
            }
        });
    });

    function SetTabId(id, event) {
        $('.TabId').val(id);
        $('.CompanyId').val('@ViewBag.CompanyId');
        checkDates(event);
    };
    $(function () {
        $('a[rel*=leanModal]').leanModal({ top: 80, closeButton: ".btnSelect,.modal_close" });
    });

    function SetDetail(id, event) {
        var validationStatus = $('#BankMobileNumber' + id).valid();
        if (!validationStatus) {
            return false;
        }
        var bankName = $('#BankName' + id).val();
        var bankNameAr = $('#BankNameArabic' + id).val();
        var bankAccountNo = $('#BankAccountNumber' + id).val();
        var bankIbanNo = $('#BankIBANNumber' + id).val();
        var mobileNo = $('#BankMobileNumber' + id).val();
        var dataToPost = {
            bankName: bankName,
            bankNameAr: bankNameAr,
            bankAccountNo: bankAccountNo,
            bankIbanNo: bankIbanNo,
            mobileNum: mobileNo
        };
        var url = siteUrl + "/CP/CompanyProfile/SendSms";
        $.blockUI({
            message: '<img src="' + siteUrl + '/Images/Gallery/ajax_loader.gif" style="width:75px; height:75"/>',
            css: { backgroundColor: '#FFFAFA', left: "47%", width: "6%" }
        });
        $.ajax({
            url: url,
            type: 'POST',
            dataType: "json",
            data: dataToPost,
            success: function (data) {
                $.unblockUI();
                if (data) {
                    alert("Success");
                } else {
                    alert("Error");
                }
            },
            error: function (e) {
                $.unblockUI();
                alert('Error=' + e.toString());
            }
        });
    }
    function hide() {
        $("#popup").hide();
    }

    function checkDates(event) {
        var comRegIssDt = $("#CommercialRegisterIssueDate").val().split('/');
        var comRegExpDt = $("#CommercialRegisterExpiryDate").val().split('/');
        var insCertIssDt = $("#InsuranceCertificateIssueDate").val().split('/');
        var insCertExpDt = $("#InsuranceCertificateExpiryDate").val().split('/');
        var chmCertIssDt = $("#ChamberCertificateIssueDate").val().split('/');
        var chmCertExpDt = $("#ChamberCertificateExpiryDate").val().split('/');
        var incCertIssDt = $("#IncomeAndZakaCertificateIssueDate").val().split('/');
        var incCertExpDt = $("#SaudilizationCertificateIssueDate").val().split('/');
        var saCerIssDt = $("#SaudilizationCertificateIssueDate").val().split('/');
        var saCerExpDt = $("#SaudilizationCertificateExpiryDate").val().split('/');
        if (comRegIssDt > comRegExpDt) {
            $("#CommercialRegisterDateValidation").text("Expiry Date cannot be less than Issue Date");
            $("#CommercialRegisterDateValidation").addClass("required");
            event.preventDefault();
            $('html,body').animate({
                scrollTop: $("#CommercialRegisterIssueDate").offset().top
            });
        }
        if (insCertIssDt > insCertExpDt) {
            $("#InsuranceCertificateDateValidation").text("Expiry Date cannot be less than Issue Date");
            $("#InsuranceCertificateDateValidation").addClass("required");
            event.preventDefault();
            $('html,body').animate({
                scrollTop: $("#InsuranceCertificateIssueDate").offset().top
            });
        }
        if (chmCertIssDt > chmCertExpDt) {
            $("#ChamberCertificateDateValidation").text("Expiry Date cannot be less than Issue Date");
            $("#ChamberCertificateDateValidation").addClass("required");
            event.preventDefault();
            $('html,body').animate({
                scrollTop: $("#ChamberCertificateIssueDate").offset().top
            });
        }
        if (incCertIssDt > incCertExpDt) {
            $("#IncomeAndZakaCertificateDateValidation").text("Expiry Date cannot be less than Issue Date");
            $("#IncomeAndZakaCertificateDateValidation").addClass("required");
            event.preventDefault();
            $('html,body').animate({
                scrollTop: $("#IncomeAndZakaCertificateIssueDate").offset().top
            });
        }
        if (saCerIssDt > saCerExpDt) {
            $("#SaudilizationCertificateDateValidation").text("Expiry Date cannot be less than Issue Date");
            $("#SaudilizationCertificateDateValidation").addClass("required");
            event.preventDefault();
            $('html,body').animate({
                scrollTop: $("#SaudilizationCertificateIssueDate").offset().top
            });
        }

    }
</script>